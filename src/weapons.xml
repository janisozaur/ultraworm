	<!-- GENERIC BULLET GLOWS AND BACKGROUNDS -->
	
	<animation name="bulletglow.animation">						
		<random>
			<dest seq="1"/>	
			<dest seq="3"/>
			<dest seq="5"/>	
			<dest seq="7"/>	
			<dest seq="8"/>
		</random>
		<alpha alpha="+-10"/>
		<goto seq="9"/>
		<alpha alpha="+-20"/>
		<goto seq="9"/>				
		<alpha alpha="+-30"/>	
		<goto seq="9"/>		
		<alpha alpha="+-40"/>		
		<random>
			<dest seq="9"/>	
			<dest seq="11"/>
			<dest seq="13"/>	
			<dest seq="15"/>	
			<dest seq="16"/>		
		</random>		
		<scale scale="+0.05"/>	
		<goto seq="16"/>		
		<scale scale="+0.1"/>
		<goto seq="16"/>			
		<goto seq="16"/>	
		<scale scale="+-0.1"/>	
	</animation>	
	
	<animation name="bulletglow.flicker.animation">						
		<random>
			<dest seq="1"/>	
			<dest seq="6"/>
			<dest seq="11"/>	
			<dest seq="16"/>	
			<dest seq="21"/>
		</random>
		<alpha alpha="+-60"/>
		<scale scale="+0.3" d="2"/>	
		<alpha alpha="+60"/>
		<scale scale="+-0.3"/>
		<goto seq="0"/>		
		<alpha alpha="+-40"/>
		<scale scale="+0.2" d="2"/>	
		<alpha alpha="+40"/>
		<scale scale="+-0.2"/>
		<goto seq="0"/>
		<alpha alpha="+-20"/>
		<scale scale="+0.1" d="2"/>	
		<alpha alpha="+20"/>
		<scale scale="+-0.1"/>
		<goto seq="0"/>		
		<alpha alpha="+20"/>
		<scale scale="+-0.1" d="2"/>	
		<alpha alpha="+-20"/>
		<scale scale="+0.1"/>
		<goto seq="0"/>			
		<alpha alpha="+40"/>
		<scale scale="+-0.2" d="2"/>	
		<alpha alpha="+-40"/>
		<scale scale="+0.2"/>
		<goto seq="0"/>
		<alpha alpha="+60"/>
		<scale scale="+-0.3" d="2"/>	
		<alpha alpha="+-60"/>
		<scale scale="+0.3"/>
		<goto seq="0"/>	
	</animation>		
	
	<animation name="bulletbackground.animation">						
		<scale scale="+-0.3"/>	
		<random>
			<dest seq="2"/>	
			<dest seq="4"/>
			<dest seq="6"/>	
			<dest seq="8"/>	
			<dest seq="9"/>		
		</random>		
		<scale scale="+0.05"/>	
		<goto seq="9"/>		
		<scale scale="+0.1"/>
		<goto seq="9"/>			
		<scale scale="+-0.05"/>	
		<goto seq="9"/>	
		<scale scale="+-0.1"/>			
		<alpha alpha="10" d="1"/>		
		<alpha alpha="20" d="1"/>		
		<alpha alpha="30" d="1"/>		
		<alpha alpha="40" d="1"/>
		<alpha alpha="50" d="1"/>				
	</animation>		



		
