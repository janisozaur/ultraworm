			<offset x="0" y="17" d="1" />
			<frame i="spriteimage.boss.earth.head.01" d="1" childXOffset="2" childYOffset="28" />
			<offset x="1" y="14" d="1" />		
			<frame i="spriteimage.boss.earth.head.01" d="1" childXOffset="-4" childYOffset="34" />
			<offset x="-2" y="17" d="1" />
			<frame i="spriteimage.boss.earth.head.01" d="1" childXOffset="4" childYOffset="24" />
			<offset x="2" y="12" d="1" />
			<frame i="spriteimage.boss.earth.head.01" d="1" childXOffset="0" childYOffset="36" />
			<offset x="0" y="18" d="1" />
			<frame i="spriteimage.boss.earth.head.01" d="1" childXOffset="2" childYOffset="26" />
			<offset x="1" y="13" d="1" />	
			<frame i="spriteimage.boss.earth.head.01" d="1" childXOffset="-4" childYOffset="34" />
			<offset x="-2" y="17" d="1" />
			<frame i="spriteimage.boss.earth.head.01" d="1" childXOffset="4" childYOffset="30" />
			<offset x="2" y="15" d="1" />
			<frame i="spriteimage.boss.earth.head.01" d="1" childXOffset="-2" childYOffset="28" />
			<offset x="-1" y="14" d="1" />

		<repeat id="roar" />
	
		<frame i="spriteimage.boss.earth.head.01" d="1" childYOffset="24" />
		<offset x="0" y="12" d="1" />
		<frame i="spriteimage.boss.earth.head.01" d="1" childYOffset="16" />
		<offset x="0" y="8" d="1" />	
		<frame i="spriteimage.boss.earth.head.01" d="1" childYOffset="10" />
		<offset x="0" y="5" d="1" />
		<frame i="spriteimage.boss.earth.head.01" d="1" childYOffset="4" />
		<offset x="0" y="2" d="1" />

		<return/>

	</animation>	
	
	
	
	<animation name="gidrah.earth.boss.head.animation">
	
		<!-- 200 frames walk -->
	
		<frame i="spriteimage.boss.earth.head.01" d="0" childYOffset="0" />
		<offset x="0" y="0" d="1" />
		<frame i="spriteimage.boss.earth.head.01" d="199" childYOffset="0" />
		
		<sub id="gidrah.earth.boss.head.roar.animation"/>
	
		<goto seq="0" />
	</animation>

	<animation name="gidrah.earth.boss.head.idle.animation">
		<offset x="0" y="0" d="0" />
		<frame i="spriteimage.boss.earth.head.01" d="0" childYOffset="0" />
	</animation>

	<animation name="gidrah.earth.boss.glow.animation">

		<!-- 200 frames walk -->

		<frame i="spriteimage.boss.earth.head.glow.02" d="2" />
		<frame i="spriteimage.boss.earth.head.glow.01" d="31" />
		
		<!--  blink -->
		
		<frame i="spriteimage.boss.earth.head.glow.02" d="2" />
		<frame i="spriteimage.boss.earth.head.glow.03" d="2" />
		<frame i="spriteimage.boss.earth.head.glow.04" d="20" />
		<frame i="spriteimage.boss.earth.head.glow.03" d="4" />
		<frame i="spriteimage.boss.earth.head.glow.02" d="5" />
		
		<frame i="spriteimage.boss.earth.head.glow.01" d="134" />		

		<!-- 100 frames roar -->

		<frame i="spriteimage.boss.earth.head.glow.02" d="2" />
		<frame i="spriteimage.boss.earth.head.glow.03" d="98" />
	
		<goto seq="0" />	
	</animation>

	<animation name="gidrah.earth.boss.glow.idle.animation">
		<frame i="spriteimage.boss.earth.head.glow.02" d="2" />
	</animation>
	
