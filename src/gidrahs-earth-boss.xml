	<animation name="gidrah.earth.boss.head.roar.animation">
	
		<!-- 100 frames roar - 4 open, 8 close, 88 roar -->
		
		<frame i="spriteimage.boss.earth.head.01" d="1" childYOffset="10" />
		<offset x="0" y="5" d="1" />
		<frame i="spriteimage.boss.earth.head.01" d="1" childYOffset="20" />
