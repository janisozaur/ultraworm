		<alpha alpha="50" d="2"/>
		<alpha alpha="40" d="3"/>
		<alpha alpha="30" d="4"/>
		<alpha alpha="20" d="5"/>
		<alpha alpha="12" d="6"/>
		<alpha alpha="8" d="7"/>
		
		<alpha alpha="0" d="200"/>

		<return/>
	</animation>


	<animation name="gidrah.earth.boss.body.death.right.animation">
		
		<!-- roar -->
		<frame i="spriteimage.boss.earth.body.back.01" d="113" />
		<shake amount="10"/>
		<frame i="spriteimage.boss.earth.body.back.01" d="37" />
				
		<!-- fall over 45 + 2 + 203 = 250 -->
		
		<offset x="-11" y="0" d="0" />
		<frame i="spriteimage.boss.earth.death.right.back.01" d="0" />
		
		<sub id="gidrah.earth.boss.death.right.fall.animation"/>
		<shake amount="10"/>
		
		<alpha alpha="255" d="165"/>
		<!-- <shake amount="16"/> -->
		<alpha alpha="255" d="30"/>
		<shake amount="32"/>
		<alpha alpha="255" d="10"/>

		<sub id="gidrah.earth.boss.death.fade.animation"/>

		<event id="1"/>

	</animation>	


	<animation name="gidrah.earth.boss.body.death.left.animation">
		
		<!-- roar -->
		<frame i="spriteimage.boss.earth.body.back.01" d="113" />
		<shake amount="10"/>
		<frame i="spriteimage.boss.earth.body.back.01" d="37" />
		
		<!-- blink -->
				
		<!-- fall over 45 + 2 + 203 = 250 -->
		
		<offset x="11" y="0" d="0" />
		<frame i="spriteimage.boss.earth.death.left.back.01" d="0" />
		
		<sub id="gidrah.earth.boss.death.left.fall.animation"/>
		<shake amount="10"/>
		
		<alpha alpha="255" d="165"/>
		<!-- <shake amount="16"/> -->
		<alpha alpha="255" d="30"/>
		<shake amount="32"/>
		<alpha alpha="255" d="10"/>

