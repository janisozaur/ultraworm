	<animation name="gidrah.earth.boss.death.right.fall.animation">
		
		<angle angle="+20" d="7"/>
		<angle angle="+30" d="6"/>
		<angle angle="+45" d="5"/>
		<angle angle="+65" d="5"/>
		<angle angle="+90" d="4"/>
		<angle angle="+120" d="4"/>
