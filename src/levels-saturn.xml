	<!-- Level definitions -->
	
	<level title="PHOEBE" colors="saturn.01.colors" world="saturn.world" scenery="saturn.scenery.base" formation="11112">
		<story setting="saturn.gidrah.setting">
			<char id="saturn.gidrah.01.char">[stats:gidrah.saturn.01.spec]</char>
			<char id="saturn.custerd-gidrah.char">we've taken them by surprise! only a few weak forces are guarding our landing area on PHOEBE.</char>
		</story>
		<story setting="saturn.gidrah.setting">
			<char id="saturn.angry.01.char">[stats:angrygidrah.saturn.01.spec]</char>
			<char id="saturn.tech-gidrah.char">we surmise that though these creatures emerge from TOXIC POOLS, they are loathe to cross them once dry! use POOLS as cover!</char>
		</story>
	</level>
	<level title="DIONE" colors="saturn.02.colors" world="saturn.world" scenery="saturn.scenery.base" formation="11122">
		<story setting="saturn.gidrah.setting">
			<char id="saturn.gidrah.02.char">[stats:gidrah.saturn.02.spec]</char>
			<char id="saturn.custerd-gidrah.char">our advantage has been lost. the titans have sent these CLEVER BLIGHTERS to seek out our facilities.</char>
		</story>
		<story setting="saturn.gidrah.setting" qualifier="!psychology">
			<char id="saturn.angry.02.char">[stats:angrygidrah.saturn.02.spec]</char>
			<char id="saturn.tech-gidrah.char">the PSYCHOLOGY DEPARTMENT has been complaining for some time about the BROKEN COFFEE MACHINE.</char>
		</story>
		<story setting="saturn.gidrah.setting" qualifier="!decoy">
			<char id="saturn.angry.02.char">[stats:angrygidrah.saturn.02.spec]</char>
			<char id="saturn.tech-gidrah.char">we hypothesise that we might be able to FOOL these smart titans into WANDERING THE WRONG WAY with some sort of DECOY.</char>
		</story>
		<story setting="saturn.gidrah.setting" qualifier="decoy">
			<char id="saturn.angry.02.char">[stats:angrygidrah.saturn.02.spec]</char>
			<char id="saturn.tech-gidrah.char">use the DECOYS we have designed to FOOL these smart titans into WANDERING THE WRONG WAY!</char>
		</story>
	</level>
	<level title="CHIRON I" colors="saturn.03.colors" world="saturn.world" scenery="saturn.scenery.base" formation="111233">
		<bosses>
			<array>
				<item>gidrah.earth.boss</item>
			</array>
		</bosses>
		<story setting="saturn.general.setting">
			<char id="saturn.custerd.char">another of those BIG ONES is on its way! i've already had to jettison TWO CONTAINERS OF COFFEE to fit my trophies in. the BOFFS are FURIOUS!</char>
		</story>
	</level>
	<level title="CHIRON II" colors="saturn.04.colors" world="saturn.world" scenery="saturn.scenery.base" formation="112233">
		<story setting="saturn.gidrah.setting">
			<char id="saturn.gidrah.03.char">[stats:gidrah.saturn.03.spec]</char>
			<char id="saturn.tech-gidrah.char">BAD NEWS, commander! it would appear that this ARMOURED BEAST is HIGHLY EXPLOSIVE!</char>
		</story>
		<story setting="saturn.gidrah.setting">
			<char id="saturn.gidrah.03.char">[stats:gidrah.saturn.03.spec]</char>
			<char id="saturn.tech-gidrah.char">fortunately this does make them HIGHLY SUSCEPTIBLE to EXPLOSIVES.</char>
		</story>
		<story setting="saturn.general.setting">
			<char id="saturn.custerd.char">i reckon we ought to TAKE THE BLIGHTERS OUT at LONG RANGE, or they'll blow down the walls!</char>
		</story>
	</level>
	<level title="THEMIS I" colors="saturn.05.colors" world="saturn.world" scenery="saturn.scenery.base" formation="1112233">
		<story setting="saturn.gidrah.setting">
			<char id="saturn.angry.03.char">[stats:angrygidrah.saturn.03.spec]</char>
			<char id="saturn.tech-gidrah.char">this gargantuan titan is simply FILLED with HIGH EXPLOSIVES and totally impervious to BLASTERS in lab tests!</char>
		</story>
		<story setting="saturn.general.setting" qualifier="capacitor,mines">
			<char id="saturn.custerd.char">CAPACITORS or MINES should do for these then - just be sure they don't get TOO CLOSE!</char>
		</story>
		<story setting="saturn.general.setting" qualifier="capacitor,!mines">
			<char id="saturn.custerd.char">CAPACITORS should do for these then - just be sure they don't get TOO CLOSE!</char>
		</story>
		<story setting="saturn.general.setting" qualifier="!capacitor,mines">
			<char id="saturn.custerd.char">lay down a carpet of MINES to get them - be sure they don't get TOO CLOSE!</char>
		</story>
		<story setting="saturn.general.setting" qualifier="!capacitor,!mines">
			<char id="saturn.custerd.char">best train our BIGGEST GUNS on them and deploy LOTS OF SCANNERS and hope they don't get TOO CLOSE!</char>
		</story>
	</level>
	<level title="THEMIS II" colors="saturn.06.colors" world="saturn.world" scenery="saturn.scenery.base" formation="1112244">
		<story setting="saturn.gidrah.setting">
			<char id="saturn.gidrah.04.char">[stats:gidrah.saturn.04.spec]</char>
			<char id="saturn.tech-gidrah.char">the general said there wouldn't be any more ghosts! this is most irregular! he promised no more ghosts!</char>
		</story>
	</level>
	<level title="ATLAS" colors="saturn.07.colors" world="saturn.world" scenery="saturn.scenery.base" formation="1112234">
		<bosses>
			<array>
				<item>gidrah.moon.boss</item>
			</array>
		</bosses>
		<story setting="saturn.general.setting">
			<char id="saturn.custerd.char">a LEVIATHAN CLASS TITAN approaches! don't let the TITANS distract you from blasting it to KINGDOM COME before it's too late!</char>
		</story>
	</level>
	<level title="MIMAS" colors="saturn.08.colors" world="saturn.world" scenery="saturn.scenery.base" formation="1111245">
		<story setting="saturn.general.setting">
			<char id="saturn.custerd.char">there's something coming on the ELF radar that appears to be FLYING OVER THE MOUNTAINS!</char>
		</story>
		<story setting="saturn.general.setting" qualifier="!laser">
			<char id="saturn.custerd.char">those blasted BOFFS had better come up with some sort of BEAM WEAPON so we can hit them above the MOUNTAINS!</char>
		</story>
		<story setting="saturn.general.setting" qualifier="laser">
			<char id="saturn.custerd.char">we can hit them even when they fly above the MOUNTAINS using LASERS!</char>
		</story>
		<story setting="saturn.general.setting">
			<char id="saturn.tech.char">we'll try to bring back a SPECIMEN in some sort of GIANT JAR.</char>
		</story>
	</level>
	<level title="TETHYS" colors="saturn.09.colors" world="saturn.world" scenery="saturn.scenery.base" formation="1111345">
		<story setting="saturn.gidrah.setting">
			<char id="saturn.angry.05.char">[stats:angrygidrah.saturn.05.spec]</char>
			<char id="saturn.tech-gidrah.char">it turns out to be quite a STRUGGLE to find a jar BIG ENOUGH, so we've TIED IT DOWN for closer examination.</char>
		</story>
		<story setting="saturn.gidrah.setting" qualifier="!laser">
			<char id="saturn.gidrah.05.char">[stats:gidrah.saturn.05.spec]</char>
			<char id="saturn.tech-gidrah.char">they fly over the mountains TOO HIGH UP for ordinary weapons, and then drop EXPLOSIVE PODS on us!</char>
		</story>
		<story setting="saturn.gidrah.setting" qualifier="laser">
			<char id="saturn.gidrah.05.char">[stats:gidrah.saturn.05.spec]</char>
			<char id="saturn.tech-gidrah.char">they fly over the mountains and can only easily be hit by LASER FIRE! then they drop EXPLOSIVE PODS on us!</char>
		</story>
	</level>
	<level title="HYPERION" colors="saturn.10.colors" world="saturn.world" scenery="saturn.scenery.base" formation="1112345">
		<bosses>
			<array>
				<item>gidrah.saturn.boss</item>
			</array>
		</bosses>
		<story setting="saturn.level.10.setting">
			<char id="saturn.boss.char">\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n scanners report incoming BEHEMOTH CLASS TITAN!</char>
			<char id="saturn.custerd.alert.char">the entire fleet is poised for takeoff to TITAN! hold off the enemy until we get the COFFEE LOADED!</char>
		</story>
	</level>

